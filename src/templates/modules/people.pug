mixin numbers(data)
	.col-md-3.people-number
		p.number= data.number
		p.name= data.name
		
		
section.container-fluid.people-bg
	section.container
		section.people-block
			div.row.people-info
				each data in [{number:'4',name:'отдела'},{number:'20+',name:'профессионалов'},{number:'7',name:'лет вместе'},{number:'150',name:'тысяч тонн в месяц'}]
					+numbers(data)
			div.row.people-button
				.col-md-offset-9.col-md-3
					button.btnm(type="button")
						span О НАШЕЙ КОМАНДЕ
